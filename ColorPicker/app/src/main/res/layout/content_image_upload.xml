<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".ImageUpload"
    tools:showIn="@layout/activity_image_upload">

    <ImageView
        android:id="@+id/IU_UploadedPhoto"
        android:layout_width="387dp"
        android:layout_height="293dp"
        android:contentDescription="@string/IU_contentDescription"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.769"
        tools:srcCompat="@tools:sample/backgrounds/scenic[16]" />

    <TextView
        android:id="@+id/IU_title"
        android:layout_width="189dp"
        android:layout_height="36dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/IU_UploadedPhoto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.062" />

    <TextView
        android:id="@+id/IU_Description"
        android:layout_width="370dp"
        android:layout_height="45dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/IU_UploadedPhoto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IU_title"
        app:layout_constraintVertical_bias="0.091" />

    <Button
        android:id="@+id/IU_gallaryButton"
        android:layout_width="373dp"
        android:layout_height="48dp"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/IU_UploadedPhoto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IU_Description"
        app:layout_constraintVertical_bias="0.129" />

    <Button
        android:id="@+id/IU_cameraButton"
        android:layout_width="374dp"
        android:layout_height="48dp"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/IU_UploadedPhoto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IU_gallaryButton"
        app:layout_constraintVertical_bias="0.196" />